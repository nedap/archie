definition
	ROLE[id1] matches {	-- Profissional de saúde
		identities matches {
			allow_archetype PARTY_IDENTITY[id2] occurrences matches {1} matches {	-- Nome do profissional
				include
					archetype_id/value matches {/(person_name)[a-zA-Z0-9_-]*\.v1/}
			}
		}
		capabilities matches {
			allow_archetype CAPABILITY[id3] matches {	-- Capacitação
				include
					archetype_id/value matches {/(individual_credentials)[a-zA-Z0-9_-]*\.v1/}
			}
		}
		contacts matches {
			CONTACT[id4] matches {	-- Contato
				addresses matches {
					allow_archetype ADDRESS[id31] occurrences matches {1} matches {	-- Endereço
						include
							archetype_id/value matches {/(address)([a-zA-Z0-9_]+)*\.v1/}
							archetype_id/value matches {/(electronic_communication)[a-zA-Z0-9_-]*\.v1/}
					}
				}
			}
		}
		relationships matches {
			PARTY_RELATIONSHIP[id5] matches {	-- Relacionamento com uma fonte pagadora
				details matches {
					ITEM_TREE[id41] occurrences matches {0..1} matches {	-- Detalhes
						items matches {
							allow_archetype CLUSTER[id42] occurrences matches {1} matches {	-- Documento de Identificação
								include
									archetype_id/value matches {/(provider_identifier)[a-zA-Z0-9_-]*\.v1/}
							}
						}
					}
				}
			}
			PARTY_RELATIONSHIP[id6] matches {	-- Relacionamento empregador/empregado
				details matches {
					ITEM_TREE[id51] occurrences matches {0..1} matches {	-- Detalhes
						items cardinality matches {1..*; ordered} matches {
							allow_archetype CLUSTER[id52] occurrences matches {1} matches {	-- Documento de Identificação
								include
									archetype_id/value matches {/(provider_identifier)[a-zA-Z0-9_-]*\.v1/}
							}
							ELEMENT[id53] occurrences matches {0..1} matches {	-- Função
								value matches {
									DV_TEXT[id54] 
								}
							}
						}
					}
				}
			}
		}
	}

