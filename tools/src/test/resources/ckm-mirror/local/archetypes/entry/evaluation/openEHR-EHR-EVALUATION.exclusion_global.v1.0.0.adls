definition
	EVALUATION[id1] matches {	-- Exclusion - global
		data matches {
			ITEM_TREE[id2] matches {
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[id3] matches {	-- Global exclusion statement
						name matches {
							DV_CODED_TEXT[id13] matches {
								defining_code matches {[ac1]}		-- Global exclusion statement (synthesised)
							}
						}
						value matches {
							DV_TEXT[id14] 
						}
					}
					ELEMENT[id12] occurrences matches {0..1} matches {	-- Comment
						value matches {
							DV_TEXT[id15] 
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[id9] matches {
				items matches {
					allow_archetype CLUSTER[id11] matches {	-- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

