definition
	OBSERVATION[id1] matches {	-- Timed 25-Foot Walk
		data matches {
			HISTORY[id2] matches {
				events cardinality matches {1..*; unordered} matches {
					POINT_EVENT[id3] occurrences matches {0..1} matches {	-- Test 1
						data matches {
							ITEM_TREE[id4] matches {
								items matches {
									ELEMENT[id5] occurrences matches {0..1} matches {	-- Zeit
										value matches {
											DV_DURATION[id22] matches {
												value matches {PTS/|PT0S..PT180S|}
											}
										}
									}
									ELEMENT[id6] occurrences matches {0..1} matches {	-- Test nicht beendet?
										value matches {
											DV_BOOLEAN[id23] matches {
												value matches {True}
											}
										}
									}
									ELEMENT[id7] matches {	-- Gründe für Nichtbeenden
										value matches {
											DV_TEXT[id24] 
										}
									}
								}
							}
						}
						state matches {
							ITEM_TREE[id10] matches {
								items matches {
									ELEMENT[id11] matches {	-- Begleitumstände
										value matches {
											DV_TEXT[id25] 
										}
									}
								}
							}
						}
					}
					POINT_EVENT[id8] occurrences matches {0..1} matches {	-- Test 2
						data matches {
							use_node ITEM_TREE[id26] /data[id2]/events[id3]/data[id4]	-- /data[id2]/events[Test 1]/data[id4]
						}
						state matches {
							use_node ITEM_TREE[id27] /data[id2]/events[id3]/state[id10]	-- /data[id2]/events[Test 1]/state[id10]
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[id12] matches {
				items matches {
					ELEMENT[id13] matches {	-- Unilaterale Gehhilfe
						value matches {
							DV_CODED_TEXT[id28] matches {
								defining_code matches {[ac1]}		-- Unilaterale Gehhilfe (synthesised)
							}
							DV_TEXT[id29] 
						}
					}
					ELEMENT[id21] matches {	-- Bilaterale Gehhilfen
						value matches {
							DV_CODED_TEXT[id30] matches {
								defining_code matches {[ac2]}		-- Bilaterale Gehhilfen (synthesised)
							}
							DV_TEXT[id31] 
						}
					}
					ELEMENT[id18] occurrences matches {0..1} matches {	-- Mehr als zwei Anläufe?
						value matches {
							DV_BOOLEAN[id32] matches {
								value matches {True}
							}
						}
					}
					ELEMENT[id19] matches {	-- Gründe für mehr als zwei Anläufe
						value matches {
							DV_TEXT[id33] 
						}
					}
				}
			}
		}
	}

