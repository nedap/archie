definition
	OBSERVATION[id1] matches {	-- Body mass index
		data matches {
			HISTORY[id2] matches {
				events cardinality matches {1..*; unordered} matches {
					EVENT[id3] matches {	-- Elke gebeurtenis
						data matches {
							ITEM_TREE[id4] matches {
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[id5] matches {	-- Body Mass Index
										value matches {
											DV_QUANTITY[id17] matches {
												property matches {[at10]}
												[magnitude, units, precision] matches {
													[{|0.0..<1000.0|}, {"kg/m2"}, {1}],
													[{|0.0..<1000.0|}, {"[lb_av]/[in_i]2"}, {1}]
												}
											}
										}
									}
									ELEMENT[id14] occurrences matches {0..1} matches {	-- *New element(en)
										value matches {
											DV_TEXT[id18] 
										}
									}
									ELEMENT[id13] occurrences matches {0..1} matches {	-- *New element(en)
										value matches {
											DV_TEXT[id19] 
										}
									}
								}
							}
						}
						state matches {
							ITEM_TREE[id15] matches {
								items matches {
									ELEMENT[id12] occurrences matches {0..1} matches {	-- *Confounding factors(en)
										value matches {
											DV_TEXT[id20] 
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[id6] matches {
				items matches {
					ELEMENT[id7] occurrences matches {0..1} matches {	-- Methode
						value matches {
							DV_CODED_TEXT[id21] matches {
								defining_code matches {[ac1]}		-- Methode (synthesised)
							}
						}
					}
					ELEMENT[id11] occurrences matches {0..1} matches {	-- Formule
						value matches {
							DV_TEXT[id22] 
						}
					}
					allow_archetype CLUSTER[id16] matches {	-- *Cluster(en)
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

