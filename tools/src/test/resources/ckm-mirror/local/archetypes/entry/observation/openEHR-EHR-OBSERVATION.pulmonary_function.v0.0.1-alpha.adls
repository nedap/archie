definition
	OBSERVATION[id1] occurrences matches {0..1} matches {	-- Pulmonary function test
		data matches {
			HISTORY[id2] matches {
				events cardinality matches {1..*; unordered} matches {
					EVENT[id3] matches {	-- Any event
						data matches {
							ITEM_TREE[id4] matches {
								items matches {
									CLUSTER[id128] matches {	-- Result details
										items matches {
											CLUSTER[id53] occurrences matches {0..1} matches {	-- Pulmonary volume result
												items matches {
													ELEMENT[id88] occurrences matches {0..1} matches {	-- Test result name
														value matches {
															DV_CODED_TEXT[id135] matches {
																defining_code matches {[ac1]}		-- Test result name (synthesised)
															}
														}
													}
													ELEMENT[id55] occurrences matches {0..1} matches {	-- Predicted result
														value matches {
															DV_QUANTITY[id136] matches {
																property matches {[at131]}
																[magnitude, units] matches {
																	[{|>=0.0|}, {"l"}],
																	[{|>=0.0|}, {"ml"}]
																}
															}
														}
													}
													ELEMENT[id54] occurrences matches {0..1} matches {	-- Actual result
														value matches {
															DV_QUANTITY[id137] matches {
																property matches {[at131]}
																[magnitude, units] matches {
																	[{|>=0.0|}, {"l"}],
																	[{|>=0.0|}, {"ml"}]
																}
															}
														}
													}
													ELEMENT[id45] occurrences matches {0..1} matches {	-- Actual/predicted ratio
														value matches {
															DV_PROPORTION[id138] matches {
																numerator matches {|>=0.0|}
																denominator matches {|0.0..100.0|}
																type matches {0, 2}
															}
														}
													}
												}
											}
											CLUSTER[id58] occurrences matches {0..1} matches {	-- Pulmonary flow rate result
												items matches {
													ELEMENT[id89] occurrences matches {0..1} matches {	-- Test result name
														value matches {
															DV_CODED_TEXT[id139] matches {
																defining_code matches {[ac2]}		-- Test result name (synthesised)
															}
														}
													}
													ELEMENT[id9] occurrences matches {0..1} matches {	-- Predicted result
														value matches {
															DV_QUANTITY[id140] matches {
																property matches {[at132]}
																[magnitude, units, precision] matches {
																	[{|0.0..60000.0|}, {"l/s"}, {0}],
																	[{|0.0..200000.0|}, {"ml/s"}, {0}],
																	[{|0.0..1000.0|}, {"l/min"}, {0}]
																}
															}
														}
													}
													ELEMENT[id59] occurrences matches {0..1} matches {	-- Actual result
														value matches {
															DV_QUANTITY[id141] matches {
																property matches {[at132]}
																[magnitude, units, precision] matches {
																	[{|0.0..60000.0|}, {"l/s"}, {0}],
																	[{|0.0..200000.0|}, {"ml/s"}, {0}],
																	[{|0.0..1000.0|}, {"l/min"}, {0}]
																}
															}
														}
													}
													ELEMENT[id123] occurrences matches {0..1} matches {	-- Actual/predicted ratio
														value matches {
															DV_PROPORTION[id142] matches {
																numerator matches {|0.0..100.0|}
																denominator matches {|0.0..100.0|}
																type matches {0, 2}
															}
														}
													}
												}
											}
											CLUSTER[id56] occurrences matches {0..1} matches {	-- Pulmonary ratio result
												items matches {
													ELEMENT[id90] occurrences matches {0..1} matches {	-- Test result name
														value matches {
															DV_CODED_TEXT[id143] matches {
																defining_code matches {[ac3]}		-- Test result name (synthesised)
															}
														}
													}
													ELEMENT[id19] occurrences matches {0..1} matches {	-- Predicted result
														value matches {
															DV_PROPORTION[id144] matches {
																numerator matches {|>=0.0|}
																denominator matches {|>=0.0|}
																type matches {0, 2}
															}
														}
													}
													ELEMENT[id57] occurrences matches {0..1} matches {	-- Actual result
														value matches {
															DV_PROPORTION[id145] matches {
																numerator matches {|>=0.0|}
																type matches {0, 2}
															}
														}
													}
												}
											}
											CLUSTER[id129] occurrences matches {0..1} matches {	-- Other result
												items matches {
													ELEMENT[id14] occurrences matches {0..1} matches {	-- Forced expiratory time (FET)
														value matches {
															DV_DURATION[id146] matches {
																value matches {PTS/|PT0S..PT300S|}
															}
														}
													}
													ELEMENT[id38] occurrences matches {0..1} matches {	-- CO transfer factor (DLCO test)
														value matches {
															DV_QUANTITY[id147] matches {
																property matches {[at133]}
															}
														}
													}
												}
											}
											allow_archetype CLUSTER[id132] matches {	-- Spirography imaging
												include
													archetype_id/value matches {/openEHR-EHR-CLUSTER\.multimedia(-[a-zA-Z0-9_]+)*\.v1/}
											}
											ELEMENT[id126] occurrences matches {0..1} matches {	-- No test result
												value matches {
													DV_BOOLEAN[id148] matches {
														value matches {True}
													}
												}
											}
											ELEMENT[id127] occurrences matches {0..1} matches {	-- Reason for no test result
												value matches {
													DV_TEXT[id149] 
												}
											}
											ELEMENT[id100] matches {	-- Interpretation
												value matches {
													DV_TEXT[id150] 
												}
											}
										}
									}
									ELEMENT[id131] occurrences matches {0..1} matches {	-- Overall interpretation
										value matches {
											DV_TEXT[id151] 
										}
									}
									ELEMENT[id102] occurrences matches {0..1} matches {	-- Comment
										value matches {
											DV_TEXT[id152] 
										}
									}
									allow_archetype CLUSTER[id134] matches {	-- Test not done
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.exclusion_exam(-[a-zA-Z0-9_]+)*\.v1/}
									}
								}
							}
						}
						state matches {
							ITEM_TREE[id32] matches {
								items matches {
									ELEMENT[id99] occurrences matches {0..1} matches {	-- Confounding factors
										value matches {
											DV_TEXT[id153] 
										}
									}
									ELEMENT[id116] occurrences matches {0..1} matches {	-- Position
										value matches {
											DV_CODED_TEXT[id154] matches {
												defining_code matches {[ac4; at117]}		-- Position (synthesised)
											}
										}
									}
									CLUSTER[id47] occurrences matches {0..1} matches {	-- Bronchial challenge
										items matches {
											ELEMENT[id48] occurrences matches {0..1} matches {	-- Agent/modality
												value matches {
													DV_TEXT[id155] 
												}
											}
											ELEMENT[id49] occurrences matches {0..1} matches {	-- Dose
												value matches {
													DV_TEXT[id156] 
												}
											}
											allow_archetype CLUSTER[id94] matches {	-- Agent detail
												include
													archetype_id/value matches {/.*/}
											}
											ELEMENT[id92] occurrences matches {0..1} matches {	-- Timing
												value matches {
													DV_CODED_TEXT[id157] matches {
														defining_code matches {[ac5]}		-- Timing (synthesised)
													}
												}
											}
											ELEMENT[id98] occurrences matches {0..1} matches {	-- Duration
												value matches {
													DV_DURATION[id158] matches {
														value matches {PDTHMS/|>=PT0S|}
													}
												}
											}
										}
									}
									CLUSTER[id50] occurrences matches {0..1} matches {	-- Bronchodilation
										items matches {
											ELEMENT[id52] occurrences matches {0..1} matches {	-- Agent
												value matches {
													DV_TEXT[id159] 
												}
											}
											use_node ELEMENT[id160] occurrences matches {0..1} /data[id2]/events[id3]/state[id32]/items[id47]/items[id49]	-- /data[id2]/events[Any event]/state[id32]/items[Bronchial challenge]/items[Dose]
											use_node ELEMENT[id161] occurrences matches {0..1} /data[id2]/events[id3]/state[id32]/items[id47]/items[id92]	-- /data[id2]/events[Any event]/state[id32]/items[Bronchial challenge]/items[Timing]
											use_node ELEMENT[id162] occurrences matches {0..1} /data[id2]/events[id3]/state[id32]/items[id47]/items[id98]	-- /data[id2]/events[Any event]/state[id32]/items[Bronchial challenge]/items[Duration]
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[id30] matches {
				items matches {
					allow_archetype CLUSTER[id31] occurrences matches {0..1} matches {	-- Device
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
					}
					allow_archetype CLUSTER[id133] matches {	-- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

terminology
	term_definitions = <
		["en"] = <
			["id1"] = <
				text = <"Pulmonary function test">
				description = <"Pulmonary function test result including spirometry and other lung volume testing.">
			>
			["id3"] = <
				text = <"Any event">
				description = <"Default, unspecified point in time or interval which may be explicitly defined in a template or at run-time.">
			>
			["id9"] = <
				text = <"Predicted result">
				description = <"Predicted pulmonary flow rate result.">
			>
			["id14"] = <
				text = <"Forced expiratory time (FET)">
				description = <"The time taken to complete  a forced expiration.">
			>
			["id19"] = <
				text = <"Predicted result">
				description = <"Predicted pulmonary test ratio result.">
			>
			["id31"] = <
				text = <"Device">
				description = <"Details of the device used to measure pulmonary function.">
			>
			["id38"] = <
