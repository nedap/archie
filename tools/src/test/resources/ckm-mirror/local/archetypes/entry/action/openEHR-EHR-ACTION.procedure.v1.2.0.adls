definition
	ACTION[id1] matches {	-- Procedure
		ism_transition matches {
			ISM_TRANSITION[id35] matches {	-- Procedure planned
				current_state matches {
					DV_CODED_TEXT[id71] matches {
						defining_code matches {[at70]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT[id72] matches {
						defining_code matches {[at35]}
					}
				}
			}
			ISM_TRANSITION[id36] matches {	-- Procedure request sent
				current_state matches {
					DV_CODED_TEXT[id73] matches {
						defining_code matches {[at70]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT[id74] matches {
						defining_code matches {[at36]}
					}
				}
			}
			ISM_TRANSITION[id39] matches {	-- Procedure postponed
				current_state matches {
					DV_CODED_TEXT[id75] matches {
						defining_code matches {[at71]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT[id76] matches {
						defining_code matches {[at39]}
					}
				}
			}
			ISM_TRANSITION[id40] matches {	-- Procedure cancelled
				current_state matches {
					DV_CODED_TEXT[id77] matches {
						defining_code matches {[at72]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT[id78] matches {
						defining_code matches {[at40]}
					}
				}
			}
			ISM_TRANSITION[id37] matches {	-- Procedure scheduled
				current_state matches {
					DV_CODED_TEXT[id79] matches {
						defining_code matches {[at73]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT[id80] matches {
						defining_code matches {[at37]}
					}
				}
			}
			ISM_TRANSITION[id69] matches {	-- Procedure commenced
				current_state matches {
					DV_CODED_TEXT[id81] matches {
						defining_code matches {[at74]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT[id82] matches {
						defining_code matches {[at69]}
					}
				}
			}
			ISM_TRANSITION[id48] matches {	-- Procedure performed
				current_state matches {
					DV_CODED_TEXT[id83] matches {
						defining_code matches {[at74]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT[id84] matches {
						defining_code matches {[at48]}
					}
				}
			}
			ISM_TRANSITION[id41] matches {	-- Procedure suspended
				current_state matches {
					DV_CODED_TEXT[id85] matches {
						defining_code matches {[at75]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT[id86] matches {
						defining_code matches {[at41]}
					}
				}
			}
			ISM_TRANSITION[id42] matches {	-- Procedure aborted
				current_state matches {
					DV_CODED_TEXT[id87] matches {
						defining_code matches {[at76]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT[id88] matches {
						defining_code matches {[at42]}
					}
				}
			}
			ISM_TRANSITION[id44] matches {	-- Procedure completed
				current_state matches {
					DV_CODED_TEXT[id89] matches {
						defining_code matches {[at77]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT[id90] matches {
						defining_code matches {[at44]}
					}
				}
			}
		}
