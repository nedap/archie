terminology
	term_definitions = <
		["ar-sy"] = <
			["id1"] = <
				text = <"*Medication(en)">
				description = <"*Details about any medication related task that is performed after ordering, such as plannling, preparation, administration, dispensing or cessation.(en)">
			>
			["id3"] = <
				text = <"*Prescription issued(en)">
				description = <"*The medication has been issued as an item on a prescription.(en)">
			>
			["id4"] = <
				text = <"*Medication dispensed(en)">
				description = <"*The medication has been dispensed, for example from a pharmacy to the patient.(en)">
			>
			["id5"] = <
				text = <"*Medication course commenced(en)">
				description = <"*The medication has been taken by, or administered to, the patient for the first time. Although in some settings this significant date may be computable as the first of several administrations, in other settings, such as primary care, specific administration dates are not readily available.(en)">
			>
			["id6"] = <
				text = <"*Medication course reviewed(en)">
				description = <"*The specific medication item has been reviewed, as planned. This is not intended to capture review of the whole medication list.(en)">
			>
			["id7"] = <
				text = <"*Dose administered(en)">
				description = <"*A single administration of the medication has taken place.(en)">
			>
			["id8"] = <
				text = <"*Medication course completed(en)">
				description = <"*The medication course has been completed as planned.(en)">
			>
			["id9"] = <
				text = <"*Medication course supply delayed(en)">
				description = <"*The medication course has not been dispensed as supply is not yet available.(en)">
			>
			["id10"] = <
				text = <"*Medication administration suspended(en)">
				description = <"*The administration of the medication has been suspended. No further doses should be given until further notice, or when the restart date or conditions have been met.(en)">
			>
			["id12"] = <
				text = <"*Prescription re-issue suspended(en)">
				description = <"*Reissuing of this prescription is not available without some intervention.(en)">
			>
			["id13"] = <
				text = <"*Medication course cancelled(en)">
				description = <"*The planned medication has been cancelled prior to any administration.(en)">
			>
			["id14"] = <
				text = <"*Scheduled medication postponed(en)">
				description = <"*A scheduled medication has been postponed prior to any administration.(en)">
			>
			["id16"] = <
				text = <"*Medication course stopped(en)">
				description = <"*The administration of this medication has been ceased after the first dose has been administered, but before intended completion of the course.  (en)">
			>
			["id17"] = <
				text = <"*Medication start date/condition set(en)">
				description = <"*The time to start this medication, or other starting condition, has been set.(en)">
			>
			["id19"] = <
				text = <"*Dose administration omitted(en)">
				description = <"*The administration of a medication has been withheld and not given. There is no expectation that it will be given later, though the next dose (if any) should be administered according to the original order.(en)">
			>
			["id21"] = <
				text = <"*Medication(en)">
				description = <"*Name of the medication, vaccine or other therapeutic/prescribable item which was the focus of the action..(en)">
			>
			["id22"] = <
				text = <"*Reason(en)">
				description = <"*The reason(s) the specific action or step was carried out. Note: This is not the reason for the medication instruction, rather the specific reason that a care step was carried out, and will often be to document some variance from the original order.(en)">
			>
			["id25"] = <
				text = <"*Comment(en)">
				description = <"*A comment on the action taken including details of any variance between the intended action and the action actually performed.(en)">
			>
			["id26"] = <
				text = <"*Sequence number(en)">
				description = <"*The sequence number specific to the action being recorded.(en)">
			>
			["id34"] = <
				text = <"*Patient guidance(en)">
				description = <"*Any guidance, instructions or advice given to the subject of care or personal carer at the time of the action.(en)">
			>
			["id36"] = <
				text = <"*Dispensed medication returned.(en)">
				description = <"*A dispensed medication is returned, or not collected, without being used.(en)">
			>
			["id40"] = <
				text = <"*Major change to order.(en)">
				description = <"*A major change to the order was required, resulting in this order being stopped and a replacement order being started.(en)">
			>
