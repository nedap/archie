definition
	EVALUATION[id1] matches {	-- Problem/Diagnosis
		data matches {
			ITEM_TREE[id2] matches {
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[id3] matches {	-- Problem/Diagnosis name
						value matches {
							DV_TEXT[id79] 
						}
					}
					ELEMENT[id10] occurrences matches {0..1} matches {	-- Clinical description
						value matches {
							DV_TEXT[id80] 
						}
					}
					ELEMENT[id13] matches {	-- Body site
						value matches {
							DV_TEXT[id81] 
						}
					}
					allow_archetype CLUSTER[id40] matches {	-- Structured body site
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.anatomical_location_clock(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.anatomical_location_relative(-[a-zA-Z0-9_]+)*\.v1/}
					}
					ELEMENT[id78] occurrences matches {0..1} matches {	-- Date/time of onset
						value matches {
							DV_DATE_TIME[id82] 
						}
					}
					ELEMENT[id4] occurrences matches {0..1} matches {	-- Date/time clinically recognised
						value matches {
							DV_DATE_TIME[id83] 
						}
					}
					ELEMENT[id6] occurrences matches {0..1} matches {	-- Severity
						value matches {
							DV_CODED_TEXT[id84] matches {
								defining_code matches {[ac1]}		-- Severity (synthesised)
							}
							DV_TEXT[id85] 
						}
					}
					allow_archetype CLUSTER[id44] matches {	-- Specific details
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[id73] occurrences matches {0..1} matches {	-- Course description
						value matches {
							DV_TEXT[id86] 
						}
					}
					ELEMENT[id31] occurrences matches {0..1} matches {	-- Date/time of resolution
						value matches {
							DV_DATE_TIME[id87] 
						}
					}
					allow_archetype CLUSTER[id47] matches {	-- Status
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.problem_status(-[a-zA-Z0-9_]+)*\.v0/}
					}
					ELEMENT[id74] occurrences matches {0..1} matches {	-- Diagnostic certainty
						value matches {
							DV_CODED_TEXT[id88] matches {
								defining_code matches {[ac2]}		-- Diagnostic certainty (synthesised)
							}
							DV_TEXT[id89] 
						}
					}
					ELEMENT[id70] occurrences matches {0..1} matches {	-- Comment
						value matches {
							DV_TEXT[id90] 
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[id33] matches {
				items matches {
					ELEMENT[id71] occurrences matches {0..1} matches {	-- Last updated
						value matches {
							DV_DATE_TIME[id91] 
						}
					}
					allow_archetype CLUSTER[id72] matches {	-- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

