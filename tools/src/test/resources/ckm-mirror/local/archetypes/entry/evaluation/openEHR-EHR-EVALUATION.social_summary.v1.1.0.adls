definition
	EVALUATION[id1] matches {	-- Social summary
		data matches {
			ITEM_TREE[id2] matches {
				items matches {
					ELEMENT[id3] occurrences matches {0..1} matches {	-- Summary
						value matches {
							DV_TEXT[id8] 
						}
					}
					allow_archetype CLUSTER[id4] matches {	-- Details
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.education(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.housing(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.household(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.employment(-[a-zA-Z0-9_]+)*\.v0/}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[id5] matches {
				items matches {
					ELEMENT[id7] occurrences matches {0..1} matches {	-- Last updated
						value matches {
							DV_DATE_TIME[id9] 
						}
					}
					allow_archetype CLUSTER[id6] matches {	-- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

