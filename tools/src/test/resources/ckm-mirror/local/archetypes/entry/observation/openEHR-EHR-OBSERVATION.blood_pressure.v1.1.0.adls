definition
	OBSERVATION[id1] matches {	-- Bloeddruk
		data matches {
			HISTORY[id2] matches {	-- Geschiedenis
				events cardinality matches {1..*; unordered} matches {
					EVENT[id7] matches {	-- *Any event(en)
						data matches {
							ITEM_TREE[id4] matches {
								items matches {
									ELEMENT[id5] occurrences matches {0..1} matches {	-- Systole
										value matches {
											DV_QUANTITY[id1060] matches {
												property matches {[at1055]}
												magnitude matches {|0.0..<1000.0|}
												precision matches {0}
												units matches {"mm[Hg]"}
											}
										}
									}
									ELEMENT[id6] occurrences matches {0..1} matches {	-- Diastole
										value matches {
											DV_QUANTITY[id1061] matches {
												property matches {[at1055]}
												magnitude matches {|0.0..<1000.0|}
												precision matches {0}
												units matches {"mm[Hg]"}
											}
										}
									}
									ELEMENT[id1007] occurrences matches {0..1} matches {	-- Gemiddelde arteriële druk
										value matches {
											DV_QUANTITY[id1062] matches {
												property matches {[at1055]}
												magnitude matches {|0.0..<1000.0|}
												precision matches {0}
												units matches {"mm[Hg]"}
											}
										}
									}
									ELEMENT[id1008] occurrences matches {0..1} matches {	-- Polsdruk
										value matches {
											DV_QUANTITY[id1063] matches {
												property matches {[at1055]}
												magnitude matches {|0.0..<1000.0|}
												precision matches {0}
												units matches {"mm[Hg]"}
											}
										}
									}
									ELEMENT[id34] occurrences matches {0..1} matches {	-- *Comment(en)
										value matches {
											DV_TEXT[id1064] 
										}
									}
								}
							}
						}
						state matches {
							ITEM_TREE[id8] matches {
								items matches {
									ELEMENT[id9] occurrences matches {0..1} matches {	-- Houding
										value matches {
											DV_CODED_TEXT[id1065] matches {
												defining_code matches {[ac1; at1002]}		-- Houding (synthesised)
											}
										}
									}
									ELEMENT[id1053] occurrences matches {0..1} matches {	-- Beïnvloedende factoren
										value matches {
											DV_TEXT[id1066] 
										}
									}
									allow_archetype CLUSTER[id1031] occurrences matches {0..1} matches {	-- Inspanning
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.level_of_exertion(-[a-zA-Z0-9_]+)*\.v1/}
									}
									ELEMENT[id1044] occurrences matches {0..1} matches {	-- Slaap/waak toestand
										value matches {
											DV_CODED_TEXT[id1067] matches {
												defining_code matches {[ac2; at1045]}		-- Slaap/waak toestand (synthesised)
											}
										}
									}
									ELEMENT[id1006] occurrences matches {0..1} matches {	-- *Tilt(en)
										value matches {
											DV_QUANTITY[id1068] matches {
												property matches {[at1056]}
												[magnitude, units, precision] matches {
													[{|-90.0..90.0|}, {"°"}, {0}],
													[{|-90.0..90.0|}, {"deg"}, {0}]
												}
											}
										}
									}
								}
							}
						}
					}
					INTERVAL_EVENT[id1043] occurrences matches {0..1} matches {	-- 24 uur gemiddelde
						math_function matches {
							DV_CODED_TEXT[id1069] matches {
								defining_code matches {[at1057]}
							}
						}
						width matches {
							DV_DURATION[id1070] matches {
								value matches {PT24H}
							}
						}
						data matches {
							use_node ITEM_TREE[id1071] /data[id2]/events[id7]/data[id4]	-- /data[Geschiedenis]/events[*Any event(en)]/data[id4]
						}
						state matches {
							use_node ITEM_TREE[id1072] /data[id2]/events[id7]/state[id8]	-- /data[Geschiedenis]/events[*Any event(en)]/state[id8]
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[id12] matches {	-- lijst structuur
				items matches {
					ELEMENT[id14] occurrences matches {0..1} matches {	-- Manchet grootte
						value matches {
