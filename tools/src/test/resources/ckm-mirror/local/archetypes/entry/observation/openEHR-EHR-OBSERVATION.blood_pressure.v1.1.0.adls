						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1/}
					}
					ELEMENT[id1036] occurrences matches {0..1} matches {	-- Methodiek
						value matches {
							DV_CODED_TEXT[id1077] matches {
								defining_code matches {[ac5]}		-- Methodiek (synthesised)
							}
						}
					}
					ELEMENT[id1039] occurrences matches {0..1} matches {	-- Gemiddelde arteriële druk formule
						value matches {
							DV_TEXT[id1078] 
						}
					}
					ELEMENT[id1055] occurrences matches {0..1} matches {	-- *Systolic pressure formula(en)
						value matches {
							DV_TEXT[id1079] 
						}
					}
					ELEMENT[id1056] occurrences matches {0..1} matches {	-- *Diastolic pressure formula(en)
						value matches {
							DV_TEXT[id1080] 
						}
					}
					ELEMENT[id1011] occurrences matches {0..1} matches {	-- Diastolisch eindpunt
						value matches {
							DV_CODED_TEXT[id1081] matches {
								defining_code matches {[ac6]}		-- Diastolisch eindpunt (synthesised)
							}
						}
					}
					allow_archetype CLUSTER[id1026] occurrences matches {0..1} matches {	-- Apparaat
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
