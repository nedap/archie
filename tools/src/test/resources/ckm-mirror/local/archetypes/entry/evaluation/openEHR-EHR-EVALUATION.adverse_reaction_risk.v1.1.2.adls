definition
	EVALUATION[id1] matches {	-- Adverse reaction risk
		data matches {
			ITEM_TREE[id2] matches {
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[id3] matches {	-- Substance
						value matches {
							DV_TEXT[id130] 
						}
					}
					ELEMENT[id64] occurrences matches {0..1} matches {	-- Status
						value matches {
							DV_CODED_TEXT[id131] matches {
								defining_code matches {[ac1]}		-- Status (synthesised)
							}
							DV_TEXT[id132] 
						}
					}
					ELEMENT[id102] occurrences matches {0..1} matches {	-- Criticality
						value matches {
							DV_CODED_TEXT[id133] matches {
								defining_code matches {[ac2]}		-- Criticality (synthesised)
							}
						}
					}
					ELEMENT[id121] occurrences matches {0..1} matches {	-- Category
						value matches {
							DV_CODED_TEXT[id134] matches {
								defining_code matches {[ac3]}		-- Category (synthesised)
							}
							DV_TEXT[id135] 
						}
					}
					ELEMENT[id118] occurrences matches {0..1} matches {	-- Onset of last reaction
						value matches {
							DV_DATE_TIME[id136] 
						}
					}
					ELEMENT[id59] occurrences matches {0..1} matches {	-- Reaction mechanism
						value matches {
							DV_CODED_TEXT[id137] matches {
								defining_code matches {[ac4]}		-- Reaction mechanism (synthesised)
							}
							DV_TEXT[id138] 
						}
					}
					ELEMENT[id7] occurrences matches {0..1} matches {	-- Comment
						value matches {
							DV_TEXT[id139] 
						}
					}
					CLUSTER[id10] matches {	-- Reaction event
						items matches {
							ELEMENT[id11] occurrences matches {0..1} matches {	-- Specific substance
								value matches {
									DV_TEXT[id140] 
								}
							}
							ELEMENT[id22] occurrences matches {0..1} matches {	-- Certainty
								value matches {
									DV_CODED_TEXT[id141] matches {
										defining_code matches {[ac5]}		-- Certainty (synthesised)
									}
								}
							}
							ELEMENT[id12] matches {	-- Manifestation
								value matches {
									DV_TEXT[id142] 
								}
							}
							ELEMENT[id13] occurrences matches {0..1} matches {	-- Reaction description
								value matches {
									DV_TEXT[id143] 
								}
							}
							ELEMENT[id28] occurrences matches {0..1} matches {	-- Onset of reaction
								value matches {
									DV_DATE_TIME[id144] 
								}
							}
							ELEMENT[id29] occurrences matches {0..1} matches {	-- Duration of reaction
								value matches {
									DV_DURATION[id145] 
								}
							}
							ELEMENT[id90] occurrences matches {0..1} matches {	-- Severity of reaction
								value matches {
									DV_CODED_TEXT[id146] matches {
										defining_code matches {[ac6]}		-- Severity of reaction (synthesised)
									}
									DV_TEXT[id147] 
								}
							}
							allow_archetype CLUSTER[id30] matches {	-- Reaction details
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.multimedia(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.anatomical_location_relative(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.symptom_sign(-[a-zA-Z0-9_]+)*\.v1/}
							}
							ELEMENT[id21] occurrences matches {0..1} matches {	-- Initial exposure
								value matches {
									DV_DATE_TIME[id148] 
								}
							}
							ELEMENT[id26] occurrences matches {0..1} matches {	-- Duration of exposure
								value matches {
									DV_DURATION[id149] 
								}
							}
							ELEMENT[id107] occurrences matches {0..1} matches {	-- Route of exposure
								value matches {
									DV_TEXT[id150] 
								}
							}
							ELEMENT[id19] occurrences matches {0..1} matches {	-- Exposure description
								value matches {
									DV_TEXT[id151] 
								}
							}
							allow_archetype CLUSTER[id97] matches {	-- Exposure details
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.citation(-[a-zA-Z0-9_]+)*\.v1/}
							}
							ELEMENT[id41] occurrences matches {0..1} matches {	-- Clinical management description
