definition
	OBSERVATION[id1] matches {	-- Fluid balance
		data matches {
			HISTORY[id2] matches {
				events cardinality matches {1..*; unordered} matches {
					INTERVAL_EVENT[id3] matches {	-- Any interval event
						data matches {
							ITEM_TREE[id4] matches {
								items matches {
									ELEMENT[id6] occurrences matches {0..1} matches {	-- Total input
										value matches {
											DV_QUANTITY[id13] matches {
												property matches {[at1]}
												[magnitude, units, precision] matches {
													[{|>=0.0|}, {"ml"}, {|>=0|}],
													[{|>=0.0|}, {"l"}, {3}],
													[{|>=0.0|}, {"[foz_us]"}, {|>=0|}]
												}
											}
										}
									}
									ELEMENT[id7] occurrences matches {0..1} matches {	-- Total output
										value matches {
											DV_QUANTITY[id14] matches {
												property matches {[at1]}
												[magnitude, units, precision] matches {
													[{|>=0.0|}, {"ml"}, {|>=0|}],
													[{|>=0.0|}, {"l"}, {3}],
													[{|>=0.0|}, {"[foz_us]"}, {|>=0|}]
												}
											}
										}
									}
									ELEMENT[id5] occurrences matches {0..1} matches {	-- Insensible loss
										value matches {
											DV_QUANTITY[id15] matches {
												property matches {[at1]}
												[magnitude, units, precision] matches {
													[{|>=0.0|}, {"ml"}, {|>=0|}],
													[{|>=0.0|}, {"l"}, {3}],
													[{|>=0.0|}, {"[foz_us]"}, {|>=0|}]
												}
											}
										}
									}
									ELEMENT[id8] occurrences matches {0..1} matches {	-- Fluid balance
										value matches {
											DV_QUANTITY[id16] matches {
												property matches {[at1]}
												[magnitude, units, precision] matches {
													[{|>=0.0|}, {"ml"}, {|>=0|}],
													[{|>=0.0|}, {"l"}, {3}],
													[{|>=0.0|}, {"[foz_us]"}, {|>=0|}]
												}
											}
										}
									}
								}
							}
						}
					}
					INTERVAL_EVENT[id9] occurrences matches {0..1} matches {	-- 24 hour total
						math_function matches {
							DV_CODED_TEXT[id17] matches {
								defining_code matches {[at2]}
							}
						}
						width matches {
							DV_DURATION[id18] matches {
								value matches {PT24H}
							}
						}
						data matches {
							use_node ITEM_TREE[id19] /data[id2]/events[id3]/data[id4]	-- /data[id2]/events[Any interval event]/data[id4]
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[id10] matches {
				items matches {
					ELEMENT[id11] occurrences matches {0..1} matches {	-- Insensible loss formula
						value matches {
							DV_TEXT[id20] 
						}
					}
					allow_archetype CLUSTER[id12] matches {	-- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

