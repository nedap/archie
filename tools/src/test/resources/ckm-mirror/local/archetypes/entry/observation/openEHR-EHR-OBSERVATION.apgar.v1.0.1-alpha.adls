definition
	OBSERVATION[id1] matches {	-- Apgar score
		data matches {
			HISTORY[id3] matches {
				events cardinality matches {1..*; unordered} matches {
					POINT_EVENT[id4] occurrences matches {0..1} matches {	-- 1 minuut
						offset matches {
							DV_DURATION[id42] matches {
								value matches {PT1M}
							}
						}
						data matches {
							ITEM_LIST[id2] matches {
								items matches {
									ELEMENT[id10] occurrences matches {0..1} matches {	-- Ademhalingsinspanning
										value matches {
											DV_ORDINAL[id43] matches {
												[value, symbol] matches {
													[{0}, {[at11]}],
													[{1}, {[at12]}],
													[{2}, {[at13]}]
												}
											}
										}
									}
									ELEMENT[id6] occurrences matches {0..1} matches {	-- Hartslag
										value matches {
											DV_ORDINAL[id44] matches {
												[value, symbol] matches {
													[{0}, {[at7]}],
													[{1}, {[at8]}],
													[{2}, {[at9]}]
												}
											}
										}
									}
									ELEMENT[id14] occurrences matches {0..1} matches {	-- Spiertonus
										value matches {
											DV_ORDINAL[id45] matches {
												[value, symbol] matches {
													[{0}, {[at15]}],
													[{1}, {[at16]}],
													[{2}, {[at17]}]
												}
											}
										}
									}
									ELEMENT[id18] occurrences matches {0..1} matches {	-- Reactie op prikkels
										value matches {
											DV_ORDINAL[id46] matches {
												[value, symbol] matches {
													[{0}, {[at19]}],
													[{1}, {[at20]}],
													[{2}, {[at21]}]
												}
											}
										}
									}
									ELEMENT[id22] occurrences matches {0..1} matches {	-- Huidskleur
										value matches {
											DV_ORDINAL[id47] matches {
												[value, symbol] matches {
													[{0}, {[at23]}],
													[{1}, {[at24]}],
													[{2}, {[at25]}]
												}
											}
										}
									}
									ELEMENT[id26] occurrences matches {0..1} matches {	-- Totaal
										value matches {
											DV_COUNT[id48] matches {
												magnitude matches {|0..10|}
											}
										}
									}
								}
							}
						}
					}
					POINT_EVENT[id27] occurrences matches {0..1} matches {	-- 2 minuten
						offset matches {
							DV_DURATION[id49] matches {
								value matches {PT2M}
							}
						}
						data matches {
							use_node ITEM_LIST[id50] /data[id3]/events[id4]/data[id2]	-- /data[id3]/events[1 minuut]/data[id2]
						}
					}
					POINT_EVENT[id28] occurrences matches {0..1} matches {	-- 3 minuten
						offset matches {
							DV_DURATION[id51] matches {
								value matches {PT3M}
							}
						}
						data matches {
							use_node ITEM_LIST[id52] /data[id3]/events[id4]/data[id2]	-- /data[id3]/events[1 minuut]/data[id2]
						}
					}
					POINT_EVENT[id29] occurrences matches {0..1} matches {	-- 5 minuten
						offset matches {
							DV_DURATION[id53] matches {
								value matches {PT5M}
							}
						}
						data matches {
							use_node ITEM_LIST[id54] /data[id3]/events[id4]/data[id2]	-- /data[id3]/events[1 minuut]/data[id2]
						}
					}
					POINT_EVENT[id32] occurrences matches {0..1} matches {	-- 10 minuten
						offset matches {
							DV_DURATION[id55] matches {
								value matches {PT10M}
							}
						}
						data matches {
							use_node ITEM_LIST[id56] /data[id3]/events[id4]/data[id2]	-- /data[id3]/events[1 minuut]/data[id2]
						}
					}
					EVENT[id38] matches {	-- Any event
						data matches {
							use_node ITEM_LIST[id57] /data[id3]/events[id4]/data[id2]	-- /data[id3]/events[1 minuut]/data[id2]
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[id30] matches {
				items matches {
					ELEMENT[id31] occurrences matches {0..1} matches {	-- Opmerkingen over de meting
						value matches {
							DV_TEXT[id58] 
						}
					}
					allow_archetype CLUSTER[id41] matches {	-- *Extension(en)
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

