definition
	CLUSTER[id1] matches {	-- Therapeutic direction
		items matches {
			ELEMENT[id58] occurrences matches {0..1} matches {	-- Direction sequence
				value matches {
					DV_COUNT[id178] matches {
						magnitude matches {|>=1|}
					}
				}
			}
			allow_archetype CLUSTER[id177] matches {	-- Dosage administration
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.dosage_administration(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.dosage_administration(-[a-zA-Z0-9_]+)*\.v1/}
			}
			ELEMENT[id67] occurrences matches {0..1} matches {	-- Direction duration
				value matches {
					DV_CODED_TEXT[id179] matches {
						defining_code matches {[ac1]}		-- Direction duration (synthesised)
					}
					DV_DURATION[id180] matches {
						value matches {|>=PT0S|}
					}
					DV_TEXT[id181] 
				}
			}
			ELEMENT[id173] occurrences matches {0..1} matches {	-- Maximum number of administrations
				value matches {
					DV_COUNT[id182] matches {
						magnitude matches {|>=1|}
					}
				}
			}
			allow_archetype CLUSTER[id91] occurrences matches {0..1} matches {	-- Direction repetition
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.timing_repetition(-[a-zA-Z0-9_]+)*\.v0/}
			}
			allow_archetype CLUSTER[id157] matches {	-- Additional details
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.conditional_medication_rules(-[a-zA-Z0-9_]+)*\.v0/}
			}
		}
	}

