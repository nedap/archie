definition
	CLUSTER[id1] matches {	-- Inspired oxygen
		items matches {
			ELEMENT[id52] occurrences matches {0..1} matches {	-- Flow rate
				value matches {
					DV_QUANTITY[id60] matches {
						property matches {[at1]}
						[magnitude, units, precision] matches {
							[{|0.0..50000.0|}, {"ml/min"}, {2}],
							[{|0.0..50.0|}, {"l/min"}, {2}]
						}
					}
				}
			}
			ELEMENT[id53] occurrences matches {0..1} matches {	-- FiO2
				value matches {
					DV_PROPORTION[id61] matches {
						numerator matches {|0.0..1.0|}
						is_integral matches {False}
						type matches {1}
					}
				}
			}
			ELEMENT[id54] occurrences matches {0..1} matches {	-- Percent O2
				value matches {
					DV_PROPORTION[id62] matches {
						numerator matches {|0.0..100.0|}
						is_integral matches {False}
						type matches {2}
					}
				}
			}
			ELEMENT[id58] occurrences matches {0..1} matches {	-- On air
				value matches {
					DV_BOOLEAN[id63] matches {
						value matches {True, False}
					}
				}
			}
			ELEMENT[id55] matches {	-- Method of oxygen delivery
				value matches {
					DV_TEXT[id64] 
				}
			}
			allow_archetype CLUSTER[id59] matches {	-- Oxygen delivery detail
				include
					archetype_id/value matches {/.*/}
			}
		}
	}

