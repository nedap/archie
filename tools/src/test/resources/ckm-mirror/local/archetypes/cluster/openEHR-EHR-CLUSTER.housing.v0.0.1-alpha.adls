definition
	CLUSTER[id1] matches {	-- Housing
		items matches {
			ELEMENT[id3] occurrences matches {0..1} matches {	-- Housing situation
				name matches {
					DV_CODED_TEXT[id18] matches {
						defining_code matches {[ac1]}		-- Housing situation (synthesised)
					}
				}
				value matches {
					DV_TEXT[id19] 
				}
			}
			ELEMENT[id5] occurrences matches {0..1} matches {	-- Description
				value matches {
					DV_TEXT[id20] 
				}
			}
			ELEMENT[id4] occurrences matches {0..1} matches {	-- Tenure
				value matches {
					DV_TEXT[id21] 
				}
			}
			allow_archetype CLUSTER[id7] matches {	-- Details
				include
					archetype_id/value matches {/.*/}
			}
			ELEMENT[id17] occurrences matches {0..1} matches {	-- Comment
				value matches {
					DV_TEXT[id22] 
				}
			}
		}
	}

