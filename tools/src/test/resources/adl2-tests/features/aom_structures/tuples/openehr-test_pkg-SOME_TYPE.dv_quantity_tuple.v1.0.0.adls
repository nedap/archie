		clinical_quantity_attr_1 matches {
			DV_QUANTITY[id2] matches {
				property matches {[at1]}
				[units, magnitude] matches {
					[{"C"}, {|>=4.0|}],
					[{"F"}, {|>=40.0|}]
				}
			}
		}
	}

