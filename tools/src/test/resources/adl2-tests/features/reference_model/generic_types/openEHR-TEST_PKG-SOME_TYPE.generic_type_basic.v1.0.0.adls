	>

	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"Illustrates the use of generic types with filled parameter types, here DV_QUANTITY.">
			keywords = <"ADL", "test">
		>
	>

	lifecycle_state = <"published">
