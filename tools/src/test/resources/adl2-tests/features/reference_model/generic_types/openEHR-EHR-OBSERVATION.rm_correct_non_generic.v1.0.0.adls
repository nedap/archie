definition
	OBSERVATION[id1] matches {	-- an Observation
		data matches {
			HISTORY[id4] matches {
				events cardinality matches {1..*; unordered} matches {
					EVENT[id3] occurrences matches {0..*} matches {	-- some Event
						data matches {
							ITEM_LIST[id5] 
						}
					}
				}
			}
		}
	}

