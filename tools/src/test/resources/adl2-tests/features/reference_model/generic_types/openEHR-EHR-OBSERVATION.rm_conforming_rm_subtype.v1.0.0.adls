	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"Test for conformance of RM subtype (here: POINT_EVENT) to statically declared RM type (EVENT). This archetype should pass validation.">
			keywords = <"ADL", "test", "RM checking">
		>
	>

