packages = <
	["CIMI_Foundation_RM"] = <
		name = <"CIMI_Foundation_RM">
		packages = <
			["Foundation"] = <
				name = <"Foundation">
				classes = <"COMPOSITION", "CONTENT", "SECTION", "ENTRY", "COMPOSITE_ENTRY", "SIMPLE_ENTRY", "CLUSTER", "VIRTUAL_CLUSTER", "BASE_ENTITY">
			>
			["Party"] = <
